{
  "name": "@holocron/rbac-nestjs",
  "version": "1.0.0",
  "description": "NestJS integration for @holocron/rbac-core with decorators, guards, and dynamic modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "rbac",
    "authorization",
    "access-control",
    "permissions",
    "roles",
    "nestjs",
    "guards",
    "decorators"
  ],
  "author": "Holocron Team",
  "license": "MIT",
  "dependencies": {
    "@holocron/rbac-core": "*"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/testing": "^10.3.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
